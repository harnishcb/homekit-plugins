{
  "private": false,
  "name": "@cuehome/homebridge-onetouch-ts",
  "version": "1.0.0",
  "description": "Control OneTouch Door Phone via Homebridge.",
  "engines": {
    "node": ">=14.x.x",
    "homebridge": ">=1.3.5"
  },
