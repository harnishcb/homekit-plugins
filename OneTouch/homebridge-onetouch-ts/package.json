    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "homebridge": "^1.6.0",
    "nodemon": "^2.0.21",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "ffmpeg-for-homebridge": "^0.1.4",
